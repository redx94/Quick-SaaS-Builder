{
  "name": "quick-saas-builder",
  "version": "1.1.1",
  "description": "An AI-powered tool for creating SaaS solutions rapidly with minimal effort and maximum efficiency.",
  "main": "index.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:dev": "vite build --mode development",
    "serve": "vite preview",
    "test": "jest",
    "lint": "eslint src --ext .jsx,.js",
    "format": "prettier --write src/**/*.{js,jsx,json,css,md}",
    "analyze": "vite build --mode analyze"
  },
  "keywords": [
    "saas",
    "ai",
    "react",
    "vite",
    "pwa",
    "builder",
    "automation",
    "quantum",
    "optimization"
  ],
  "author": "Reece Dixon",
  "license": "MIT",
  "dependencies": {
    "@radix-ui/react-scroll-area": "^1.2.0",
    "@radix-ui/react-tooltip": "^1.1.3",
    "@vitejs/plugin-react": "^4.0.0",
    "axios": "^1.6.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "lucide-react": "^0.447.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-query": "^3.39.3",
    "react-router-dom": "^6.4.0",
    "sonner": "^1.5.0",
    "tailwind-merge": "^2.5.3",
    "tailwindcss-animate": "^1.0.7",
    "vite-plugin-pwa": "^0.14.0",
    "zustand": "^4.1.0"
  },
  "devDependencies": {
    "@testing-library/react": "^14.0.0",
    "autoprefixer": "^10.4.0",
    "babel-jest": "^29.6.0",
    "eslint": "^8.42.0",
    "eslint-plugin-react": "^7.32.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.12",
    "jest": "^29.6.0",
    "postcss": "^8.4.0",
    "prettier": "^3.0.0",
    "tailwindcss": "^3.3.0",
    "vite": "^4.4.0"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "ecmaVersion": 2021,
      "sourceType": "module"
    },
    "rules": {
      "react/prop-types": "off",
      "no-unused-vars": [
        "warn"
      ],
      "react/react-in-jsx-scope": "off"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ]
}
